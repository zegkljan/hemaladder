{
  "name": "hemaladder",
  "version": "0.0.1",
  "description": "A customizable HEMA ladder static web app.",
  "productName": "HEMA Ladder",
  "author": "Jan Å½egklitz",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "make-ladder": "python src/build/process-data.py src/build/resources/data public/data",
    "prebuild": "npm run make-ladder && npm run generate-icons",
    "postbuild": "npm run generate-meta",
    "predev": "npm run prebuild",
    "generate-icons": "node src/build/generate-icons.mjs",
    "generate-meta": "node src/build/setup-meta.mjs"
  },
  "dependencies": {
    "@unhead/vue": "^1.8.10",
    "date-fns": "^2.29.1",
    "pinia": "^2.0.17",
    "quasar": "^2.7.5",
    "vue": "^3.0.0",
    "vue-country-flag-next": "^2.3.2",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@quasar/app-vite": "^1.0.5",
    "@quasar/cli": "^2.0.0",
    "@quasar/extras": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.7.1",
    "xml2json": "^0.12.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
