{
  "name": "hemaladder",
  "version": "0.0.1",
  "description": "A customizable HEMA ladder static web app.",
  "productName": "HEMA Ladder",
  "author": "Jan Å½egklitz",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "make-ladder": "python src/build/process-data.py src/build/resources/data public/data",
    "prebuild": "cp -r src/build/resources/brand/assets public && npm run make-ladder",
    "postbuild": "npm run generate-meta",
    "predev": "npm run prebuild",
    "pregenerate-icons": "node src/build/logo-svg2png.mjs",
    "generate-icons": "npx icongenie generate --profile src/build/icon-profile.json",
    "postgenerate-icons": "rm src/build/logo.png",
    "generate-meta": "node src/build/setup-meta.mjs"
  },
  "dependencies": {
    "@unhead/vue": "^1.9.11",
    "date-fns": "^3.6.0",
    "pinia": "^2.1.7",
    "quasar": "^2.16.4",
    "vue": ">=3.0.0",
    "vue-country-flag-next": "^2.3.2",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@quasar/app-vite": "^1.9.3",
    "@quasar/cli": "^2.4.1",
    "@quasar/extras": "^1.16.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": "^22 || ^20 || ^18",
    "npm": ">= 10.8.0",
    "yarn": ">= 1.21.1"
  }
}
